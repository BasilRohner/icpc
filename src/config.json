{
    "whitelist": {
        "cpp": {
            "ext": "cpp"
        },
        "hashell": {
            "ext": "hs"
        },
        "python": {
            "ext": "py"
        }
    },
    "skip": [
        "#include",
        "using",
        "typedef"
    ],
    "skipignore": [
        "Template"
    ],
    "prefix": "$",
    "targetdir": "/Library/Application Support/Code/User/snippets/icpc.code-snippets",
    "sourcedir": "../templates/",
    "tabsze": 4
}